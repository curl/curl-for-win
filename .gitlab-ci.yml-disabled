---
# https://docs.gitlab.com/ce/ci/yaml/README.html

variables:
  GIT_STRATEGY: fetch
  GIT_DEPTH: "8"
  DO_NOT_TRACK: 1

build:
  image: debian:testing
  variables:
    CC: mingw-clang
  script:
    - docker images --digests
    - ./_ci-linux-debian.sh
  artifacts:
    expire_in: 13 weeks
    paths:
      - "*-*-mingw*.*"
  only:
    - master
    - main
    - dev-gitlab
    - test-gitlab
